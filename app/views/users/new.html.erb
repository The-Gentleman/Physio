<div class="user-new-container">
    <h1>Sign up here</h1>

    <%= form_for(@user) do |f| %>
        <%= f.label :username %>
        <%= f.text_field :username, required: true %>
        
        <%= f.label :password %>
        <%= f.password_field :password, required: true %>
        
        <%= f.submit "Sign Up" %><br>
    <% end %>
</div>